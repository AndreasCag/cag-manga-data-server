{
  "name": "data-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand",
    "watch-node": "NODE_ENV=development nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "build-ts": "tsc",
    "rm-dist": "rimraf ./dist",
    "dev": "PORT=3000 npm run rm-dist && npm run build-ts && concurrently \"npm run watch-ts\" \"npm run watch-node\"",
    "debug": "PORT=3000 npm run rm-dist && npm run build-ts && node dist/server.js",
    "migrate": "sequelize db:migrate",
    "setup-test-db": "npm run test-db:drop && npm run test-db:create",
    "test-db:drop": "sequelize db:drop --env=test",
    "test-db:create": "sequelize db:create --env=test && sequelize db:migrate --env=test",
    "lint": "tslint --project ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/supertest": "^2.0.8",
    "concurrently": "^4.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.0",
    "@types/node": "^12.0.8",
    "@types/validator": "^10.11.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.0",
    "fast-glob": "^3.0.3",
    "module-alias": "^2.2.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.9.0",
    "sequelize-cli": "^5.5.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "winston": "^3.2.1"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
